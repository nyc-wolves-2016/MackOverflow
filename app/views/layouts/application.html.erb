    <%= javascript_include_tag 'https://use.fontawesome.com/4f1036d1c5.js' %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
  </head>

  <body>
    <header id="top-nav">
      <div id="brand">Mackoverflow</div>
      <nav>
        <ul>
          <li></li>
          <li></li>
        </ul>
      </nav>
      <%= react_component 'SearchBoxView'%>
      <i className="fa fa-search"></i>
    </header>
    <%= yield %>
  </body>
</html>
